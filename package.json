{
  "name": "@stoic/app-desktop",
  "version": "1.0.0",
  "description": "Stoic for Desktop",
  "main": "build/main.js",
  "author": "Naveen M Vinayan",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start": "concurrently npm:watch:build npm:run:desktop",
    "build": "npm-run-all watch:build copy-files",
    "watch:build": "npx tsc --build tsconfig.json --watch",
    "run:desktop": "electron . --env dev",
    "test": "jest",
    "watch:test": "npx jest --watchAll",
    "run:app": "node ./build/main.js",
    "translate": "npx tsc --build tsconfig.json",
    "watch:app": "npx nodemon ./build/main.js --watch ./build",
    "copy-files": "copyfiles -u 1 ./src/**/*.html src/**/*.css ./build",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@mui/x-data-grid": "^5.6.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react-redux": "^7.1.23",
    "accounting": "^0.4.1",
    "ag-grid-community": "^27.1.0",
    "ag-grid-react": "^27.1.0",
    "async-mutex": "^0.3.2",
    "dayjs": "^1.10.7",
    "electron-window-state": "^5.0.3",
    "eslint-plugin-jest": "^26.1.1",
    "fs-extra": "^10.0.0",
    "nanoid": "^3.3.1",
    "nanotimer": "^0.3.15",
    "papaparse": "^5.3.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-data-grid": "^7.0.0-beta.11",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-sortable-tree": "^2.8.0",
    "recharts": "^2.1.9",
    "sprintf-js": "^1.1.2",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/accounting": "^0.4.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/react-datepicker": "^4.4.1",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "electron": "^16.0.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
