{
  "name": "@stoic/app-desktop",
  "version": "1.0.0",
  "description": "Stoic for Desktop",
  "main": "build/main.js",
  "author": "Naveen Vinayan",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start": "concurrently npm:watch:build npm:run:desktop",
    "build": "npm-run-all watch:build copy-files",
    "run:desktop": "electron . --env dev",
    "test": "jest",
    "watch:test": "npx jest --watchAll",
    "run:app": "node ./build/main.js",
    "translate": "npx tsc --build tsconfig.json",
    "watch:build": "npx tsc --build tsconfig.json --watch",
    "watch:app": "npx nodemon ./build/main.js --watch ./build",
    "copy-files": "copyfiles -u 1 ./src/**/*.html src/**/*.css ./build"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react-redux": "^7.1.23",
    "async-mutex": "^0.3.2",
    "dayjs": "^1.10.7",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^10.0.0",
    "nanotimer": "^0.3.15",
    "papaparse": "^5.3.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "sprintf-js": "^1.1.2",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "electron": "^16.0.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
